<html>
  <head>
    <title>Queen Elizabeth OPD Routine Report</title>
    <%= stylesheet_link_tag "mateme" %>
    <%= stylesheet_link_tag fancy_or_high_contrast_touch %>
    <link href="/stylesheets/cohort.css" media="screen" rel="stylesheet" type="text/css" />
    <link href="/stylesheets/cohortprint.css" media="print" rel="stylesheet" type="text/css" />

    <script type="text/javascript">
      tt_cancel_destination = '/people';
    </script>

  </head>
  <body>
    <form id="frmMain" action="/cohort/cohort" method="post">
      <input type="hidden" id="start_date" name="start_date" value="<%#= @specified_period[0].to_date.strftime('%d-%m-%Y') %>" />
      <input type="hidden" id="end_date" name="end_date" value="<%#= @specified_period[1].to_date.strftime('%d-%m-%Y') %>" />
    </form>

    <div>
      <fieldset class="main_fieldset">
        <legend class="main_legend" style="color:white; background-color:green;">
          Queen Elizabeth OPD Routine Report
        </legend>

        <table cellspacing="0" width="100%">
          <tr>
            <td align="center" colspan="4" class="padding-bottom-20">
              <label class="main_heading" style="float:left;">
                <b><i>Start Date:</i></b> <%= @specified_period[0].to_time.strftime("%a %d %b, %Y %H:%M") %>
              </label>
              <label class="main_heading" style="float:right;">
                <b><i>End Date:</i></b> <%= @specified_period[1].to_time.strftime("%a %d %b, %Y %H:%M") %>
              </label>
            </td>
          </tr>
          <tr style="color:white; background-color:green;">
            <th width="25%" class="cellleft celltop cellbottom main-table-cell" style="width:220px;">
              <i>&nbsp;</i>
            </th>
            <th class="cellleft celltop cellbottom main-table-cell" style="width:200px;">
              <i>07:30 - 16:30</i>
            </th>
            <th class="cellleft celltop cellbottom main-table-cell" style="width:200px;">
              <i>16:30 - 07:30</i>
            </th>
            <th class="cellleft celltop cellbottom main-table-cell">
              <i>Total</i>
            </th>
          </tr>
        </table>

        <div style="width:100%; height:360px; overflow:auto;">
          <table cellspacing="0" width="100%">
            <tr>
              <td class="cellleft cellbottom main-table-cell topic" style="width:220px;">
                <i>ADMISSION</i>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="width:200px; font-size:1.5em;">
                <%=  @admissions0730_1630 %>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="width:200px; font-size:1.5em;">
                <%=  @admissions1630_0730 %>
              </td>
              <td class="cellleft cellbottom cellright main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @admissions0730_1630 + @admissions1630_0730 %>
              </td>
            </tr>
            <tr>
              <td class="cellleft cellbottom main-table-cell topic">
                <i>DISCHARGES</i>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @discharged0730_1630 %>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @discharged1630_0730 %>
              </td>
              <td class="cellleft cellbottom cellright main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @discharged0730_1630 + @discharged1630_0730 %>
              </td>
            </tr>
            <tr>
              <td class="cellleft cellbottom main-table-cell topic">
                <i>REFERRALS</i>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @referrals0730_1630 %>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @referrals1630_0730 %>
              </td>
              <td class="cellleft cellbottom cellright main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @referrals0730_1630 + @referrals1630_0730 %>
              </td>
            </tr>
            <tr>
              <td class="cellleft cellbottom main-table-cell topic">
                <i>MATERNAL DEATHS</i>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @deaths0730_1630 %>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @deaths1630_0730 %>
              </td>
              <td class="cellleft cellbottom cellright main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @deaths0730_1630 + @deaths1630_0730 %>
              </td>
            </tr>
            <tr>
              <td class="cellleft cellbottom main-table-cell topic">
                <i>CESARIAN SECTIONS</i>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @cesarean0730_1630 %>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @cesarean1630_0730 %>
              </td>
              <td class="cellleft cellbottom cellright main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @cesarean0730_1630 + @cesarean1630_0730 %>
              </td>
            </tr>
            <tr>
              <td class="cellleft cellbottom main-table-cell topic">
                <i>SVDS</i>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @svds0730_1630 %>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @svds1630_0730 %>
              </td>
              <td class="cellleft cellbottom cellright main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @svds0730_1630 + @svds1630_0730 %>
              </td>
            </tr>
            <tr>
              <td class="cellleft cellbottom main-table-cell topic">
                <i>VACUUM EXTRACTION</i>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @vacuum0730_1630 %>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @vacuum1630_0730 %>
              </td>
              <td class="cellleft cellbottom cellright main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @vacuum0730_1630 + @vacuum1630_0730 %>
              </td>
            </tr>
            <tr>
              <td class="cellleft cellbottom main-table-cell topic">
                <i>BREECH</i>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @breech0730_1630 %>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @breech1630_0730 %>
              </td>
              <td class="cellleft cellbottom cellright main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @breech0730_1630 + @breech1630_0730 %>
              </td>
            </tr>
            <tr>
              <td class="cellleft cellbottom main-table-cell topic">
                <i>RUPTURED UTERUS</i>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @ruptured0730_1630 %>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @ruptured1630_0730 %>
              </td>
              <td class="cellleft cellbottom cellright main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @ruptured0730_1630 + @ruptured1630_0730 %>
              </td>
            </tr>
            <tr>
              <td class="cellleft cellbottom main-table-cell topic">
                <i>TRIPLETS</i>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @triplets0730_1630 %>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @triplets1630_0730 %>
              </td>
              <td class="cellleft cellbottom cellright main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @triplets0730_1630 + @triplets1630_0730 %>
              </td>
            </tr>
            <tr>
              <td class="cellleft cellbottom main-table-cell topic">
                <i>TWINS</i>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @twins0730_1630 %>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @twins1630_0730 %>
              </td>
              <td class="cellleft cellbottom cellright main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @twins0730_1630 + @twins1630_0730 %>
              </td>
            </tr>
            <tr>
              <td class="cellleft cellbottom main-table-cell topic">
                <i>BBA</i>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @bba0730_1630 %>
              </td>
              <td class="cellleft cellbottom main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @bba1630_0730 %>
              </td>
              <td class="cellleft cellbottom cellright main-table-cell" align="center" style="font-size:1.5em;">
                <%=  @bba0730_1630 + @bba1630_0730 %>
              </td>
            </tr>
          </table>
        </div>

        <table cellspacing="0" width="100%" style="background-color:green; color:white;">
          <tr>
            <td class="cellleft celltop cellbottom main-table-cell topic" style="width:220px;">
              <!--b><i>TOTALS</i></b-->
            </td>
            <td class="cellleft celltop cellbottom main-table-cell" style="width:200px;" align="center" style="font-size:1.5em;">
              &nbsp;
            </td>
            <td class="cellleft celltop cellbottom main-table-cell" style="width:200px;" align="center" style="font-size:1.5em;">
              &nbsp;
            </td>
            <td class="cellleft celltop cellbottom cellright main-table-cell" align="center" style="font-size:1.5em;">
              &nbsp;
            </td>
          </tr>
        </table>
      </fieldset>

    </div>

    <div style="z-index:11;bottom:15px; height: 64px; width: 890px; left: -4px; right: -66px; position: absolute;">
      <button onclick="window.location = '/cohort';" class="button navButton red"
              id="cancelButton" style="position: absolute; z-index: 100; "><span>Cancel</span></button>

      <button onclick="window.location = '/cohort';" class="button navButton gray"
              id="backButton" ><span>Back</span></button>
    </div>
  </body>
</html>