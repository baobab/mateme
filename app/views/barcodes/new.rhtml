<style type="text/css"> 
  #SHIFT, 
  #space, 
  #lessthan, 
  #greaterthan, 
  #star,
  #abc,
  #date,
  #minus,
  #plus,
  #decimal,
  #slash,
  #equals,
  #comma,
  #percent { display: inline; }
  #Unknown, #unknown { display: none; }
</style>

<script>
  tt_cancel_destination = "/people?active_tab=tab2";
</script>

<% form_tag barcodes_path do -%>
  <label for="barcode">Barcode</label>
  <%= text_field_tag(:barcode, nil, :helpText => "Enter Barcode", :textCase => 'mixed') %><br>
  <%= text_field_tag(:label, nil, :helpText => "Enter Label") %><br>
  <%= submit_tag value="Submit"%>
<% end %>
